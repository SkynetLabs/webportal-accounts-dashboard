{
  "name": "accounts-dashboard",
  "version": "1.0.0",
  "private": true,
  "description": "Accounts Dashboard",
  "author": "Skynet Labs",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "develop:secure": "dotenv -e .env.development -- gatsby develop --https -p=443",
    "start": "yarn develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
